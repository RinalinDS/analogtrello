{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-console": "warn",
    "prefer-const": "warn",
    "quotes": [
      "error",
      "single"
    ],
    "indent": [
      "warn",
      2
    ],
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ],
        "newlines-between": "always-and-inside-groups"
      }
    ],
    "@typescript-eslint/no-unused-vars": [
      "warn",
      { "varsIgnorePattern": "React", "argsIgnorePattern": "^_" }
    ]
  }
}